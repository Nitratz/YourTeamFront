{
    "name": "project",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@material-ui/core": "3.5.1",
        "@material-ui/icons": "3.0.1",
        "babel-eslint": "9.0.0",
        "react": "16.5.2",
        "react-cookies": "0.1.0",
        "react-dom": "16.5.2",
        "react-redux": "5.1.1",
        "react-router": "4.3.1",
        "react-router-dom": "4.3.1",
        "react-scripts": "2.0.4",
        "redux": "4.0.1",
        "redux-thunk": "2.3.0",
        "socket.io-client": "2.2.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "eslintConfig": {
        "extends": "airbnb-base",
        "env": {
            "es6": true,
            "browser": true
        },
        "rules": {
            "brace-style": [
                "error",
                "stroustrup"
            ],
            "comma-dangle": [
                "error",
                "never"
            ],
            "no-unused-vars": [
                "warn"
            ],
            "no-var": [
                "off"
            ],
            "one-var": [
                "off"
            ]
        }
    },
    "devDependencies": {
        "eslint": "5.6.0",
        "eslint-config-airbnb": "17.1.0",
        "eslint-config-prettier": "3.3.0",
        "eslint-plugin-react": "7.11.1",
        "prettier-eslint": "8.8.2",
        "redux-devtools-extension": "2.13.5"
    }
}
